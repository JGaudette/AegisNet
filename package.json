{
  "name": "aegis-net",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest --forceExit --config jestconfig.json",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "ts-node src/index.ts",
    "dev-web": "nodemon dist/app.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": { "registry": "https://npm.pkg.github.com/" },
  "files": ["dist/**/*"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nishgowda/Aegis.git"
  },
  "keywords": ["AegisNet"],
  "author": "Nish Gowda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nishgowda/Aegis/issues"
  },
  "homepage": "https://github.com/nishgowda/Aegis#readme",
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/async-redis": "^1.1.1",
    "@types/supertest": "^2.0.10",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "async-redis": "^1.1.7"
  }
}
